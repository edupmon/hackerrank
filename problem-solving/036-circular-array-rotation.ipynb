   "source": [
    "def circularArrayRotation(a, k, queries):\n",
    "    k = k % len(a)\n",
    "    a = a[-k:] + a[:-k]\n",
    "    return [a[i] for i in queries]"
   ]
  },
  {
   "cell_type": "code",
