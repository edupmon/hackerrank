   "source": [
    "def utopianTree(n):\n",
    "    ret_val = 1\n",
    "    for i in range(n):\n",
    "        if i % 2 == 0:\n",
    "            ret_val *= 2\n",
    "        else:\n",
    "            ret_val += 1\n",
    "    return ret_val"
   ]
  },
